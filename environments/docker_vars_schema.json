{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "server_names_list": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "instancetype": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": [
                  "uf",
                  "deploymentserver",
                  "licensemaster",
                  "full",
                  "clustermanager",
                  "search",
                  "indexers",
                  "shdeployer",
                  "shc"
                ]
              }
            }
          }
        },
        "required": ["name", "instancetype"]
      }
    },
    "required": ["server_names_list"]
  }
  